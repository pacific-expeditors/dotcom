{
  "name": "pacific-expeditors",
  "version": "0.0.1",
  "description": "Pacific Expeditors Website",
  "main": "server.js",
  "repository": "git@github.com:pacific-expeditors/dotcom-front-end.git",
  "author": "David Sims",
  "license": "UNLICENSED",
  "engines": {
    "node": "8.1.3"
  },
  "scripts": {
    "start": "node server.js",
    "dev-server": "nodemon server.js --exec babel-node",
    "dev-client": "webpack-dashboard -- webpack-dev-server",
    "dev": "yarn run dev-server && yarn run dev-client",
    "build": "rm -rf dist && webpack -p --config webpack.production.js",
    "test": "yarn run mocha-all && yarn run flow",
    "mocha": "mocha ./app/components/tests/Home.js -r mock-local-storage -r ignore-styles -r jsdom-global/register -r dotenv/config --compilers js:babel-register",
    "mocha-all": "mocha ./app/components/tests/*.js -r mock-local-storage -r ignore-styles -r jsdom-global/register -r dotenv/config --compilers js:babel-register --recursive",
    "mocha-watch": "mocha -w ./app/components/*/tests -r dotenv/config -r mock-local-storage --compilers js:babel-register -r jsdom-global/register -r dotenv/config -r ignore-styles -r isomorphic-fetch",
    "flow": "flow"
  },
  "dependencies": {
    "apollo-client": "^1.9.0-0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.2",
    "cf-graphql": "^0.4.3",
    "chai": "^4.1.0",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "flow-bin": "^0.50.0",
    "graphiql": "^0.11.2",
    "graphql": "^0.10.3",
    "graphql-tools": "^1.1.0",
    "graphql-type-factory": "^0.1.0",
    "history": "^4.6.3",
    "html-webpack-plugin": "^2.29.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "mock-local-storage": "^1.0.2",
    "node-fetch": "^1.7.1",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-apollo": "^1.4.3",
    "react-dom": "^15.6.1",
    "react-flow-types": "^0.1.1",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.2.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.5.1"
  }
}
